function reverse(a)
  let size = len(a)
  let max = int(math_floor(size / 2))
  for let i = 0; i < max; i = i + 1
    let n = size - i - 1
    let t = a[i]
    a[i] = a[n]
    a[n] = t
  end
end

let start = os_clock()

let N = 20000000
let list = []

for let i = 0; i < N; i = i + 1 push(list, i) end

reverse(list)

let seconds = os_clock() - start
print "elapsed: " + seconds + "s | " + list[0]
