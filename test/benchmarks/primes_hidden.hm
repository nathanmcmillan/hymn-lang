let math = {}

math.is_prime = function(n)
  if n < 2 return false end
  for i = 2, i < n - 1
    if n % i == 0 return false end
  end
  return true
end

let start = os.clock()

let N = 30000
let primes = []

for i = 0, i < N
  if math.is_prime(i)
    push(primes, i)
  end
end

let seconds = os.clock() - start
echo "elapsed: " + seconds + "s | " + primes[-1]
