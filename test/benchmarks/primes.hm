function is_prime(n)
  if n < 2 return false end
  for let i = 2; i < n - 1; i = i + 1
    if n % i == 0 return false end
  end
  return true
end

let start = os_clock()

let N = 30000
let primes = []

for let i = 0; i < N; i = i + 1
  if is_prime(i)
    push(primes, i)
  end
end

let seconds = os_clock() - start
print "elapsed: " + seconds + "s"
