# begin
# 2
# 4
# 8
# end
# begin
# a -> 2
# b -> 4
# c -> 8
# end
# begin
# 10
# 12
# 14
# end
# begin
# d -> 10
# e -> 12
# end
# begin
# 4
# 8
# end

let table = { a: 2, b: 4, c: 8 }

echo "begin"
for value in table {
  echo value
}
echo "end"

echo "begin"
for k, value in table {
  echo k + " -> " + value
}
echo "end"

echo "begin"
for value in { d: 10, e: 12, f: 14 } {
  echo value
}
echo "end"

echo "begin"
for k, value in { d: 10, e: 12, f: 14 } {
  if k == "f" { break }
  echo k + " -> " + value
}
echo "end"

echo "begin"
for value in table {
  if value == 2 { continue }
  echo value
}
echo "end"
