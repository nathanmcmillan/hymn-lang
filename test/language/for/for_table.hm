let table = { a: 2, b: 4, c: 8 }

echo "begin"
for value in table
  echo value
end
echo "end"

echo "begin"
for k, value in table
  echo k + " -> " + value
end
echo "end"

echo "begin"
for value in { d: 10, e: 12, f: 14 }
  echo value
end
echo "end"

echo "begin"
for k, value in { d: 10, e: 12, f: 14 }
  if k == "f" break end
  echo k + " -> " + value
end
echo "end"

echo "begin"
for value in table
  if value == 2 continue end
  echo value
end
echo "end"
