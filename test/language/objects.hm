-- My cat is Mittens and says Meow!
-- My dog is Cookies and says Bork!

function meow()
  return "Meow!"
end

function bork()
  return "Bork!"
end

function cat(name)
  return { name: name, says: meow }
end

function dog(name)
  return { name: name, says: bork }
end

let k = cat("Mittens")
let d = dog("Cookies")

echo "My cat is " + k.name + " and says " + k.says()
echo "My dog is " + d.name + " and says " + d.says()
