# {}
# { "a": 2, "b": 4, "c": 6 }
# { "a": 2, "b": 4, "c": 6, "d": 8 }
# { "a\"b": 2, "c\"d": 4, "e'f": 6, "g'h": 8, "ijkl": 10, "x\\z": 12 }

{
  set table = {}
  echo table
}

{
  set table = { a: 2, b: 4, c: 6 }
  echo table
}

{
  set table = { "a": 2, "b": 4, 'c': 6, 'd': 8 }
  echo table
}

{
  set table = { "a\"b": 2, 'c"d': 4, "e'f": 6, 'g\'h': 8, "ij" "kl": 10, "x\\z": 12 }
  echo table
}
