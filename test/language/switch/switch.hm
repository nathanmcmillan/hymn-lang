# first
# second
# or
# else

let x = 3

switch x
case 3
  print "first"
case 5
  print "bad"
else
  print "bad"
end

switch x
case 5
  print "bad"
case 3
  print "second"
else
  print "bad"
end

switch x
case none
  print "bad"
case true
  print "bad"
case 2 or 3 or 4
  print "or"
else
  print "bad"
end

switch x
case 4
  print "bad"
case 5
  print "bad"
else
  print "else"
end
