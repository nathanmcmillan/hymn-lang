-- ["foo", ["bar", [..]]]
-- ["bar", ["foo", [..]]]
-- { name: "foo", point: { name: "bar", point: { .. } } }
-- { name: "bar", point: { name: "foo", point: { .. } } }
-- [1, [2, [3, [..]]]]
-- [2, [3, [1, [..]]]]
-- [3, [1, [2, [..]]]]

let a = ["foo"]
let b = ["bar"]

push(a, b)
push(b, a)

echo a
echo b

clear(a)
clear(b)

let c = { name: "foo" }
let d = { name: "bar" }

c.point = d
d.point = c

echo c
echo d

clear(c)
clear(d)

let e = [1]
let f = [2]
let g = [3]

push(e, f)
push(f, g)
push(g, e)

echo e
echo f
echo g

clear(e)
clear(f)
clear(g)
