# tail call
func tail(i) {
  if i < 0 { return 100 }
  return tail(i - 1)
}
echo tail(10)

# pop two
{
  set x = 1
  set y = 2
  echo x + y
}

# pop n
{
  set x = 2
  set y = 3
  set z = 4
  set w = 5
  set v = 6
  echo x + y + z + w + v
}

# negate constant
set a = [2, 4, 6]
echo a[-1]

# double negate constant
set b = [2, 4, 6]
echo b[- - 1]

# add two local
{
  set x = 2
  set y = 4
  set z = x + y
  echo z
}

# if less
if 0 < 1 { echo "X" } else { echo "no" }

# if true
if true { echo "Y" } else { echo "no" }

# if false
if false { echo "no" } else { echo "Z" }

# if not
if !false { echo "W" } else { echo "no" }

# increment
set x = 1
{
  set z = x + 255
  echo z
}

# increment local
{
  set x = 1
  set z = x + 255
  echo z
}

# increment and set local
{
  set x = 1
  x = x + 255
  echo x
}
