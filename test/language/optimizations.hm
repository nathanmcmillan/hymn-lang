-- tail call
function tail(i)
  if i < 0 return 100 end
  return tail(i - 1)
end
echo tail(10)

-- pop two
begin
  let x = 1
  let y = 2
  echo x + y
end

-- pop n
begin
  let x = 2
  let y = 3
  let z = 4
  let w = 5
  let v = 6
  echo x + y + z + w + v
end

-- negate constant
let a = [2, 4, 6]
echo a[-1]

-- double negate constant
let b = [2, 4, 6]
echo b[- - 1]

-- add two local
begin
  let x = 2
  let y = 4
  let z = x + y
  echo z
end

-- if less
if 0 < 1 echo "X" else echo "no" end

-- if true
if true echo "Y" else echo "no" end

-- if false
if false echo "no" else echo "Z" end

-- if not
if !false echo "W" else echo "no" end

-- increment
let x = 1
begin
  let z = x + 255
  echo z
end

-- increment local
begin
  let x = 1
  let z = x + 255
  echo z
end

-- increment and set local
begin
  let x = 1
  x = x + 255
  echo x
end
