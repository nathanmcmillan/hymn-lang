echo b[- - 1]

# add two local
{
  let x = 2
  let y = 4
  let z = x + y
  echo z
}

# if less
if 0 < 1 { echo "X" } else { echo "no" }

# if true
if true { echo "Y" } else { echo "no" }

# if false
if false { echo "no" } else { echo "Z" }

# if not
if !false { echo "W" } else { echo "no" }

# increment
let x = 1
{
  let z = x + 255
  echo z
}

# increment local
{
  let x = 1
  let z = x + 255
  echo z
}

# increment and set local
{
  let x = 1
  x = x + 255
  echo x
}
