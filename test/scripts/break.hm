#!/usr/bin/env hymn

let x = 0

while x < 8
  print("x == " + x)
  let y = 0
  while y < 4
    let z = y + 1
    print("z == " + z)
    if y == 2
      break
    end
    y = y + 1
  end
  if x == 4
    break
  end
  x = x + 1
end

print("done")

for let i = 0; i < 10; i = i + 1
  if i == 2
    break
  end
  print("i: " + i)
end

print("----------")

for let x = 0; x < 2; x = x + 1
  if x == 10
    break
  end
  print("x: " + x)
end
