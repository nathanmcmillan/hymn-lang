function last_index(array, value)
  let size = len(array)
  let i = size - 1
  while i >= 0
    if array[i] == value
      return i
    end
    i = i - 1
  end
  return -1
end

function upper(s)
  let new = ""
  let size = len(s)
  for let i = 0; i < size; i = i + 1
    let c = s[i]
    switch c
    case 'a' c = 'A'
    case 'b' c = 'B'
    case 'f' c = 'F'
    case 'o' c = 'O'
    end
    new = new + c
  end
  return new
end

function abs(n)
  if n < 0 return -n end
  return n
end

function min(a, b)
  if a < b return a end
  return b
end

function max(a, b)
  if a > b return a end
  return b
end

function clamp(n, a, b)
  return min(max(n, a), b)
end
