function last_index(array, value)
  let size = len(array)
  let i = size - 1
  while i >= 0
    if array[i] == value
      return i
    end
    i = i - 1
  end
  return -1
end

function upper(string)
  new = ""
  let size = len(string)
  let i = 0
  while i < size
    let c = string[i]
    if c == 'a' c = 'A'
    elif c == 'b' c = 'B'
    end
    push(new, c)
  end
  return new
end

function abs(n)
  if n < 0 return -n end
  return n
end

function min(a, b)
  if a < b return a end
  return b
end

function max(a, b)
  if a > b return a end
  return b
end
